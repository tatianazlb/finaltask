<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <display-name>ParseServletdisplay</display-name>
        <servlet-name>/parseServlet</servlet-name>
        <servlet-class>epam.zlobich.task6.servlet.ParseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>/parseServlet</servlet-name>
        <url-pattern>/parseServlet</url-pattern>
    </servlet-mapping>

    <display-name>FirstProject</display-name>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>password.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <taglib>
            <taglib-uri>customtags</taglib-uri>
            <taglib-location>/WEB-INF/tld/custom.tld</taglib-location>
        </taglib>
    </jsp-config>
</web-app>